= link_to 'Export All (.csv)', report_results_path(@report, params: @params, format: 'csv')
%table
  %tr
    - @fields.each do |field|
      %th
        = sort_link @q, (field[:title] || field[:name])
        
  - @results.each do |result|
    %tr
      - @fields.each do |field|
        %td
          = result[field[:name].to_sym]

= paginate @results